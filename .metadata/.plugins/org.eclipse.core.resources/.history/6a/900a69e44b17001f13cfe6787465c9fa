package com.ds.academy.client.member.vo;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

public class MemberVO {
    private int mNum;
    private String mAthoNum;
    private String email;
    private String mPwd;
    private String name;
    private String gender;
    private String mobile;

    // Default constructor
    public MemberVO() {}

    // Parameterized constructor
    public MemberVO(int mNum, String mAthoNum, String email, String mPwd, String name, String gender, String mobile) {
        this.mNum = mNum;
        this.mAthoNum = mAthoNum;
        this.email = email;
        this.mPwd = mPwd;
        this.name = name;
        this.gender = gender;
        this.mobile = mobile;
    }

    // Getter and setter methods
    public int getMNum() {
        return mNum;
    }

    public void setMNum(int mNum) {
        this.mNum = mNum;
    }

    public String getMAthoNum() {
        return mAthoNum;
    }

    public void setMAthoNum(String mAthoNum) {
        this.mAthoNum = mAthoNum;
    }

    public String getEmail() {
        return email;
    }

    public void setEmail(String email) {
        this.email = email;
    }

    public String getMPwd() {
        return mPwd;
    }

    public void setMPwd(String mPwd) {
        this.mPwd = mPwd;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getGender() {
        return gender;
    }

    public void setGender(String gender) {
        this.gender = gender;
    }

    public String getMobile() {
        return mobile;
    }

    public void setMobile(String mobile) {
        this.mobile = mobile;
    }
}
