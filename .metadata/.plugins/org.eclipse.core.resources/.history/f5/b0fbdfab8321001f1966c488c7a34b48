	$(function(){
		// 해당 클래스를 가진 요소를 찾습니다.
		var youtubeButton = document.querySelector('#ytp-id-48');

		// 요소가 존재하는지 확인한 후 삭제 또는 숨김 처리를 합니다.
		if (youtubeButton) {
			youtubeButton.style.display = 'none'; // 요소 숨김 처리
		}
	})

</script>
	
